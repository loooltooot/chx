generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  id       String @id @unique @db.VarChar(255)
  title    String @db.VarChar(255)
  content  String
  views    Int    @default(0)
  karma    Int    @default(0)
  category String @db.VarChar(255)
}
